.slot-wrap .back-icon {
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,);
  background-size: 48rpx;
  margin-left: 32rpx;
}
.u-placeholder {
  background: #fff;
}
.main-wrap {
  padding-bottom: env(safe-area-inset-bottom);
}
.message-wrap {
  background: #f8f8f8;
  padding-bottom: 150rpx;
  min-height: 92vh;
}
.message-wrap .m-nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 36rpx 77rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  background: #fff;
  border-bottom: solid 20rpx #f8f8f8;
}
.message-wrap .m-nav .nav-item {
  text-align: center;
}
.message-wrap .m-nav .nav-item .m-icon {
  margin-bottom: 12rpx;
}
.message-wrap .m-nav .nav-item .m-icon .icon {
  width: 96rpx;
  height: 96rpx;
}
.message-wrap .m-nav .nav-item .m-icon .icon.icon-star {
  background: url(data:image/png;base64,);
  background-size: 100%;
}
.message-wrap .m-nav .nav-item .m-icon .icon.icon-follow {
  background: url(data:image/png;base64,);
  background-size: 100%;
}
.message-wrap .m-nav .nav-item .m-icon .icon.icon-comment {
  background: url(data:image/png;base64,);
  background-size: 100%;
}
.message-wrap .m-nav .nav-item .u-text {
  font-size: 29rpx;
  color: #000;
}


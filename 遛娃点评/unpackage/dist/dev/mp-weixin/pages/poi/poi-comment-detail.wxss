.slot-wrap .back-icon {
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,);
  background-size: 48rpx;
  margin-left: 32rpx;
}
.content {
  background: #fff;
}
.content > .u-tt {
  height: 100rpx;
  line-height: 100rpx;
  text-indent: 32rpx;
  font-size: 32rpx;
  color: #555;
}
.content .m-detail {
  padding: 0 32rpx;
  box-sizing: border-box;
  border-bottom: solid 20rpx #f8f8f8;
}
.content .m-detail .m-text {
  font-size: 29rpx;
  color: #222;
  line-height: 48rpx;
}
.content .m-detail .m-images {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.content .m-detail .m-images .image-item {
  width: 218rpx;
  height: 218rpx;
  border-radius: 12px;
  background: #D8D8D8;
  margin-top: 20rpx;
}


.slot-wrap .back-icon {
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
  margin-left: 44rpx;
}
.main-part {
  margin: 0 32rpx;
}
.main-part .m-part {
  margin-bottom: 21rpx;
}
.main-part .m-part .main-tt {
  height: 100rpx;
  font-size: 32rpx;
  color: #3d3d3d;
  line-height: 100rpx;
  font-weight: 700;
}
.main-part .m-part .main-date {
  height: 100rpx;
  background: #f8f8f8;
  border-radius: 12rpx;
  font-size: 29rpx;
  color: #555;
  line-height: 100rpx;
  text-align: center;
}
.main-part .m-part.part-one .introduce {
  font-size: 29rpx;
  line-height: 40rpx;
  color: #555;
  text-justify: inter-ideograph;
  text-align: justify;
}
.main-part .m-part.part-two .date-item {
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 29rpx;
}
.main-part .m-part.part-two .date-item.first-child {
  border-bottom: solid 1rpx #f2f2f2;
}
.main-part .m-part.part-two .date-item .left {
  color: #999;
}
.main-part .m-part.part-two .date-item .right {
  color: #222;
}
.main-part .m-part.part-three .u-text {
  font-size: 29rpx;
  color: #555;
}
.main-part .m-part.part-four .ticket-item {
  padding: 40rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 29rpx;
}
.main-part .m-part.part-four .ticket-item.border-bottom {
  border-bottom: solid 1rpx #f2f2f2;
}
.main-part .m-part.part-four .ticket-item .left {
  color: #999;
  width: 164rpx;
}
.main-part .m-part.part-four .ticket-item .right {
  color: #222;
  line-height: 40rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-justify: inter-ideograph;
  text-align: justify;
}
.main-part .m-part.part-five {
  font-size: 29rpx;
}
.main-part .m-part.part-five .sub-tt {
  font-weight: 700;
  color: #222;
}
.main-part .m-part.part-five .u-text {
  margin: 20rpx 0 40rpx 0;
  color: #555;
}
.main-part .m-part.part-five .other-item .tt {
  font-weight: 700;
  color: #222;
  margin-bottom: 20rpx;
}
.main-part .m-part.part-five .other-item .link {
  color: #4A90E2;
  margin-bottom: 40rpx;
}


.m-wrap {
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.m-wrap .u-icon {
  width: 88rpx;
}
.m-wrap .u-icon.introduce {
  background: url(data:image/png;base64,) no-repeat center 32rpx;
  background-size: 40rpx;
}
.m-wrap .u-icon.ticket {
  background: url(data:image/png;base64,) no-repeat center 32rpx;
  background-size: 40rpx;
}
.m-wrap .u-icon.time {
  background: url(data:image/png;base64,) no-repeat center 32rpx;
  background-size: 40rpx;
}
.m-wrap .u-icon.location {
  background: url(data:image/png;base64,) no-repeat center 32rpx;
  background-size: 40rpx;
}
.m-wrap .m-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: solid 1rpx #F2F2F2;

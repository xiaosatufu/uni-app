.m-wrap {
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 32rpx 0 40rpx;
  box-sizing: border-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.m-wrap .info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 29rpx;
  color: #222;
}
.m-wrap .info .icon {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
  margin-right: 23rpx;
}
.m-wrap .arrow {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,);
  background-size: 40rpx;
}

